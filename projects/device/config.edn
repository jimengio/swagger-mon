
{
  :port 7800
  :routes [
    {
      :path "devices"
      :get {:type :file, :file "devices.json"}
      :next [
        {
          :path "categories"
          :get {:type :file, :file "categories.json"}
        }
        {
          :path ":deviceId"
          :delete {:code 200, :type :file, :file "ok.json"}
        }
      ]
    }
  ]
}
